#include<iostream>
#include<cstdio>
#include<string>
#include<cstring>
#include<cmath>
#include<algorithm>
#include <cstdlib>
int a[105];
double d[100000005];
using namespace std;

int main()
{
	
	int a,l,sum = 1;
	string st;
	cin>>a>>st;
	l =  st.length();
	for(int i=a;i>0;)
	{
		sum *= i;
		i -= l;
	}
	printf("%d\n",sum);
	
	return 0;
}
